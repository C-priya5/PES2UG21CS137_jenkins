pipeline {
    agent any
    stages {
         stage('Clone repository') {
             steps {
                 checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/C-priya5/PES2UG21CS137_jenkins']]])
             }
         }
        stage('Build') {
            steps {
                build 'PES2UG21CS137-1'
                sh 'g++ new.cpp -o output'
            }
        }
        stage('Test') {
            steps {
                
                sh './output'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploy'
            }
        }
    }
    post {
        failure {
            echo 'Pipeline failed'
        }
    }
}